{
  "description": "FastRP with log-transformed edge weights for better discrimination",
  "configs": [
    {
      "name": "fullgraph_128_q3_log",
      "description": "FastRP 128D q=3 with log-transformed weights",
      "fastrp": {
        "dim": 128,
        "q": 3,
        "projection": "sparse",
        "normalize": true
      },
      "reduction": {
        "method": "none"
      },
      "preprocessing": {
        "weight_transform": "log"
      },
      "output_suffix": "fullgraph_128_q3_log"
    },
    {
      "name": "fullgraph_128_q3_sqrt",
      "description": "FastRP 128D q=3 with sqrt-transformed weights",
      "fastrp": {
        "dim": 128,
        "q": 3,
        "projection": "sparse",
        "normalize": true
      },
      "reduction": {
        "method": "none"
      },
      "preprocessing": {
        "weight_transform": "sqrt"
      },
      "output_suffix": "fullgraph_128_q3_sqrt"
    }
  ]
}
